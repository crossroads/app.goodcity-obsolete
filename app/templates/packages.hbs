<div class="row ">
  {{#if itemTypeId}}
    {{view "my-container-view" elementId="my_container_view" noPackages=noPackages}}
    {{view "addComponentOfItem" controller=controller packages=allPackages}}
  {{else}}
    <p>
      <i class="icon-empty-items fa fa-info-circle fa-5x"></i>
      <p class="no-items">Please choose Item Type first!</p>
    </p>
  {{/if}}
</div>
